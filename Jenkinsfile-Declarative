pipeline{
  agent any
  environment{
    PATH="/opt/maven/bin:$PATH"
  }
  stages{
    stage("Hello World"){
      steps{
        sh """
          echo "Hello Wolrd from Omkar"
          whoami
          pwd
        """
      }
    }
    stage("Build"){
      steps{
        sh 'mvn clean install'
      }
    }
  }
}
